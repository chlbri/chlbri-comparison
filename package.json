{
  "name": "gitpod-typescript",
  "version": "0.0.1",
  "description": "GitPod for Typescript",
  "author": "chlbri <bri_lvi@icloud.com>",
  "license": "MIT",
  "private": true,
  "files": [
    "dist"
  ],
  "scripts": {
    "tsc": "tsc",
    "build": "pnpm run tsc && pnpm run lint",
    "test": "jest",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "eslint": "eslint \"{src/**/*,App}.{ts,tsx}\"",
    "lint": "pnpm run prettier && pnpm run eslint --fix",
    "rm": "./node_modules/.bin/rimraf -rf node_modules pnpm.lock",
    "rinit": "pnpm run rm && pnpm cache clean && pnpm run pnpm",
    "publish": "pnpm run rm && pnpm run clean && pnpm run pnpm",
    "clean": "pnpm cache clean",
    "pnpm": "pnpm install",
    "git-push": "git push --tags"
  },
  "dependencies": {
    "dequal": "^2.0.2"
  }
}
